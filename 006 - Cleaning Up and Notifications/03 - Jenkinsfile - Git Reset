pipeline {
    agent { 
		dockerfile { 
			filename 'DockerFile_Git'
       			dir '006 - Cleaning Up and Notifications'
		}
	}
    stages {
        stage('build') {
            steps {
                sh "data > data.txt"
            }
        }
	stage('reset') {
            steps {
                sh "git reset --hard"
		sh "data > data.txt"
            }
        }
    }
}
