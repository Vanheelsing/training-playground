pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
   agent {
    		docker { 
			image 'maven:latest' 
		}
    }
    agent {
    		docker { 
			image 'npm:latest' 
		}
    }
    stages {
    	stage("Maven"){
		steps{
			sh 'maven --version'
		}
	}
	stage("Npm"){
		steps{
			sh 'npm --version'
		}
	}
    }
}
