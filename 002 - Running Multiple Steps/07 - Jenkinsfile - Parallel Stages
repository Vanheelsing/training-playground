pipeline {
    agent none
    stages {
    	stage('Build'){
		parallel {
    			stage("Maven"){
				agent{docker { image 'maven:latest' }}
				steps{
					sh 'mvn --version'
				}
			}
			stage("Npm"){
				agent{docker { image 'node:latest' }}
				steps{
					sh 'npm --version'
				}
			}
    		}
	}
	stage('Final'){
		steps{
			sh 'npm --version'
		}
    	}
}
